{
    "typescript.validate.enable": false,
    "workbench.tree.indent": 18,
    "intelephense.diagnostics.undefinedMethods": false,
    "intelephense.diagnostics.undefinedType": false,
    "editor.fontFamily": "'Jetbrains Mono','Source Code Pro',Consolas,'Jetbrains Mono', 'Courier New', monospace",
    "editor.lineHeight": 21,
    "editor.fontLigatures": false,
    "editor.suggestSelection": "first",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.4,
    "javascript.preferences.jsxAttributeCompletionStyle": "none",
    "editor.parameterHints.enabled": false,
    // "editor.parameterHints": false,
    "breadcrumbs.enabled": false,
    "workbench.editor.showTabs": true,
    //
    // ======================bracketPairColorization start ===========================================
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
      //light color
      "editorBracketHighlight.foreground1": "#0d53ad", ////97041d
      "editorBracketHighlight.foreground2": "#6f0185",
      "editorBracketHighlight.foreground3": "#2f4f4f",
      "editorBracketHighlight.foreground4": "#A0522D",
      "editorBracketHighlight.foreground5": "#008000",
      "editorBracketHighlight.foreground6": "#008080",
      "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
      // //
      // dark color
      //
      // "editorBracketHighlight.foreground1": "#c09bc7", ////97041d
      // "editorBracketHighlight.foreground2": "#dab675", //dab675
      // "editorBracketHighlight.foreground3": "#47acac",
      // "editorBracketHighlight.foreground4": "#c2b3bd",
      // "editorBracketHighlight.foreground5": "#cf7951",
      // "editorBracketHighlight.foreground6": "#6784bb", //6784bb
      // "editorBracketHighlight.unexpectedBracket.foreground": "#a4f3e0"
    },
    // "emmet.triggerExpansionOnTab": true,
